{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "千奇百怪计算器",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tab-bar/home.png",
        "selectedIconPath": "static/tab-bar/home-active.png",
        "pagePath": "pages/home/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tab-bar/mine.png",
        "selectedIconPath": "static/tab-bar/mine-active.png",
        "pagePath": "pages/about/index",
        "text": "关于"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/home/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/about/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "subPackages": [
    {
      "root": "packages/math-calculator",
      "pages": [
        {
          "path": "index/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "数学计算器"
          }
        }
      ]
    },
    {
      "root": "packages/blood-type-calculator",
      "pages": [
        {
          "path": "index/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "血型计算器"
          }
        }
      ]
    }
  ]
}